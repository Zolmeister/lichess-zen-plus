{
  "manifest_version": 2,
  "name": "BridgeBase Dark Theme",
  "version": "0.1",
  "web_accessible_resources": [
    "theme.css"
  ],
  "permissions": [
    "*://www.bridgebase.com/*"
  ],
  "content_scripts": [{
      "matches": ["*://www.bridgebase.com/*"],
      "css": ["theme.css"]
  }],
  "browser_specific_settings": {
    "gecko": {
      "id": "bridgebase_dark_theme@zolmeister"
    }
  }
}
